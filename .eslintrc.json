{
    "env": {
      "commonjs": true,
      "es6": true,
      "node": true,
      "jest/globals": true
    },
    "extends": ["airbnb-base", "prettier"],
    "globals": {
      "Atomics": "readonly",
      "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
      "ecmaVersion": 2018
    },
    "rules": {
      "prettier/prettier": "error",
      "max-len": ["error", 80],
      "no-console": 0,
      "func-names": 0
    },
    "plugins": ["prettier", "jest"]
  }
  